{
  "compilerOptions": {
    "composite": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "strict": true,
    "sourceMap": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@comp/*": ["components/*"],
      "@style/*": ["styles/*"],
      "@layout/*": ["layouts/*"],
      "@util/*": ["utils/*"],
      "@type/*": ["types/*"],
      "@api/*": ["apis/*"],
      "@cos/*": ["composes/*"],
      "@store/*": ["stores/*"],
      "@/*": ["./*"]
    }
  },
  "include": ["src/**/*", "vite.config.ts"]
}
